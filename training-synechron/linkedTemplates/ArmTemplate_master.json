{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "training-synechron"
		},
		"LS_ADLS_azuretraningsynedls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_azuretraningsynedls'"
		},
		"LS_ASQL_retaildb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ASQL_retaildb'"
		},
		"LS_ADLS_azuretraningsynedls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuretraningsynedls.dfs.core.windows.net/"
		},
		"LS_ASQL_retaildb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sandboxtraining.database.windows.net"
		},
		"LS_ASQL_retaildb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "retaildb"
		},
		"LS_ASQL_retaildb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rootadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "training-synechron_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_azuretraningsynedls_accountKey": {
						"value": "[parameters('LS_ADLS_azuretraningsynedls_accountKey')]"
					},
					"LS_ASQL_retaildb_password": {
						"value": "[parameters('LS_ASQL_retaildb_password')]"
					},
					"LS_ADLS_azuretraningsynedls_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_azuretraningsynedls_properties_typeProperties_url')]"
					},
					"LS_ASQL_retaildb_properties_typeProperties_server": {
						"value": "[parameters('LS_ASQL_retaildb_properties_typeProperties_server')]"
					},
					"LS_ASQL_retaildb_properties_typeProperties_database": {
						"value": "[parameters('LS_ASQL_retaildb_properties_typeProperties_database')]"
					},
					"LS_ASQL_retaildb_properties_typeProperties_userName": {
						"value": "[parameters('LS_ASQL_retaildb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}